{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1"
  },
  "description": "a food delivery service app for a restaurant",
  "main": "app.js",
  "scripts": {
    "start:api": "nodemon --exec babel-node -- app.js",
    "start:client": "webpack-dev-server",
    "build": "webpack --mode=production && babel index.js -d dist",
    "test:client": "jest --config=jest.config.json",
    "test": "jest --config=jest.config.json --coverage",
    "test-client:watch": "jest --watch",
    "start": "node dist/index.js",
    "coveralls": "jest --colors --coverage && cat ./coverage/lcov.info | coveralls",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "./node_modules/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcdomain/REACTJS-Fast-Food-Fast.git"
  },
  "keywords": [
    "in-memoryData"
  ],
  "author": "Marcus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcdomain/REACTJS-Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/marcdomain/REACTJS-Fast-Food-Fast#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.4",
    "fetch-mock": "^7.3.0",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.7.0",
    "react-testing-library": "^5.4.4",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "toastr": "^2.1.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3"
  }
}
