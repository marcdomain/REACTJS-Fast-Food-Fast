{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1"
  },
  "description": "a food delivery service app for a restaurant",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- app.js",
    "test": "npm run createTables && nyc --reporter=html --reporter=text mocha server/tests/*.js --exit --compilers js:babel-core/register",
    "test:dev": "cross-env NODE_ENV=test npm run createTables && cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha server/tests/*.js --exit --compilers js:babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "createTables": "babel-node -- ./server/db/createTables",
    "build:dev-server": "webpack-dev-server",
    "build": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcdomain/REACTJS-Fast-Food-Fast.git"
  },
  "keywords": [
    "in-memoryData"
  ],
  "author": "Marcus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcdomain/REACTJS-Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/marcdomain/REACTJS-Fast-Food-Fast#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "pg": "^7.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "style-loader": "^0.23.1",
    "swagger-ui-express": "^4.0.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3"
  }
}
